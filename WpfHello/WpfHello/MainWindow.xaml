<Window x:Class="WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="98,122,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="210,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="361" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="98,215,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="210,215,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.016,0.152" Width="337" Height="160"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox Canvas.Left="90" Canvas.Top="12" Width="170" x:Name="myTextBox" />
    </Canvas>
</Window>
